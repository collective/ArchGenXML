ArrayField(
<dtml-var "basefield">
<dtml-in "options.keys()">
<dtml-let key="_['sequence-item']">
<dtml-if "len(options[key].split('\n'))>1">
    <dtml-var "key">=<dtml-var "indent(options[key], 1, skipFirstRow=True, skipBlank=True)">,
<dtml-else>
    <dtml-var "key">=<dtml-var "options[key]">,
</dtml-if>
</dtml-let>
</dtml-in>
),
